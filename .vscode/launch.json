{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Run Backtrack Code",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/javaScriptAlgos/backtrack.js",
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Merge Sort",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/javaScriptAlgos/mergeSort.js",
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Find Height of Binary Tree",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/javaScriptAlgos/tree.js",
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Graph",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/javaScriptAlgos/graphs.js",
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "realScenarios",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/typeScriptAlgos/realScenarios/src/driver.ts",
      "runtimeArgs": ["-r", "ts-node/register"],
      "sourceMaps": true,
      "outFiles": ["${workspaceFolder}/**/*.js"]
    }
  ]
}
